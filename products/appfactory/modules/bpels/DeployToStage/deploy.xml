<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:ArtifactCreateCallbackService="http://localhost:9763/services/ArtifactCreateCallbackService" xmlns:build.core.appfactory.carbon.wso2.org="http://build.core.appfactory.carbon.wso2.org" xmlns:deploy.core.appfactory.carbon.wso2.org="http://deploy.core.appfactory.carbon.wso2.org" xmlns:sample="http://wso2.org">
  <process name="sample:DeployToStage">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="sample:DeployToStage" port="DeployToStagePort"/>
    </provide>
    <provide partnerLink="createPL">
      <service name="ArtifactCreateCallbackService:ArtifactCreateCallbackService" port="ArtifactCreateCallbackBinding"/>
    </provide>
    <invoke partnerLink="deployPL">
      <service name="deploy.core.appfactory.carbon.wso2.org:ApplicationDeployer" port="ApplicationDeployerHttpsSoap11Endpoint"/>
    </invoke>
    <invoke partnerLink="createPL">
      <service name="build.core.appfactory.carbon.wso2.org:ArtifactCreator" port="ArtifactCreatorHttpsSoap11Endpoint"/>
    </invoke>
  </process>
</deploy>